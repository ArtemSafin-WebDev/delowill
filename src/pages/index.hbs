<section class="intro">
    <div class="container">
        <div class="intro__content">
            <div class="intro__bg">
                <img src="img/intro-bg.jpg" alt="" class="intro__bg-image">
            </div>
            <div class="intro__top-row">
                <div class="intro__left-col">
                    <div class="intro__tags">
