{{#extend "layout"}}
{{#content "main"}}


<section class="intro">
    <div class="container">
        <div class="intro__content">
            <div class="intro__bg">
                <img src="img/intro-bg.jpg" alt="" class="intro__bg-image">
            </div>
            <div class="intro__top-row">
                <div class="intro__left-col">
                    <div class="intro__tags">
                        <a href="#" class="intro__tag">
                            Парикмахерская
                        </a>
                    </div>
                    <div class="intro__updated-at">
                        Обновлено 14.04.2021
                    </div>
                </div>
                <div class="intro__right-col">
                    <div class="intro__controls">
                        <a href="#" class="intro__controls-btn">
                            <svg width="14" height="14" aria-hidden="true" class="icon-star">
                                <use xlink:href="#star"></use>
                            </svg>
                        </a>
                        <a href="#" class="intro__controls-btn">
                            <svg width="14" height="14" aria-hidden="true" class="icon-stats">
                                <use xlink:href="#stats"></use>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
            <div class="intro__franchise-info">
                <img src="img/chio-logo.png" alt="" class="intro__franchise-logo">
                <h1 class="intro__franchise-heading">
                    Чио-Чио
                </h1>
                <div class="intro__franchise-description">
                    <p>
                        Японская парикмахерская эконом-формта
                    </p>
                </div>
                <div class="intro__franchise-buttons">
                    <div class="intro__franchise-buttons-col">
                        <a href="#" class="intro__franchise-button intro__franchise-button--filled">
                            Написать представителю
                            <svg width="14" height="14" aria-hidden="true" class="icon-message">
                                <use xlink:href="#message"></use>
                            </svg>
                        </a>
                        <div class="intro__franchise-button-details">
                            <p>
                                Отвечает в течении 2 минут
                            </p>
                        </div>
                    </div>
                    <div class="intro__franchise-buttons-col">
                        <a href="#" class="intro__franchise-button">
                            Франчайзи на карте
                            <svg width="14" height="14" aria-hidden="true" class="icon-location">
                                <use xlink:href="#location"></use>
                            </svg>
                        </a>
                        <div class="intro__franchise-button-details">
                            <p>
                                524 партнера в 145 городах
                            </p>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<section class="franchise-block need-to-know">
    <div class="container">
        <div class="franchise-block__content need-to-know__content">
            <div class="inner-container">
                <h2 class="section-heading">
                    Что нужно знать об этой франшизе
                </h2>
                <div class="need-to-know__blocks">
                    <div class="need-to-know__block">
                        <h3 class="need-to-know__block-heading">
                            Почему это лучшая сфера для открытия бизнеса
                        </h3>
                        <ul class="need-to-know__block-list">
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/1.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Большой спрос
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        90% населения стригутся раз в месяц
                                    </div>
                                </div>
                            </li>
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/1.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Возврат клиентов
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        Клиенты возвращаются каждый месяц
                                    </div>
                                </div>
                            </li>
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/1.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Нет сильных конкурентов
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        Много советских парикмахерских с низким уровнем сервис
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <div class="need-to-know__block">
                        <h3 class="need-to-know__block-heading">
                            Почему эта франшиза лучшая в своей сфере
                        </h3>
                        <ul class="need-to-know__block-list">
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/2.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Много успешных кейсов
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        500 открыто. 100 открыли больше двух точек
                                    </div>
                                </div>
                            </li>
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/2.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Своя школа парикмахеров
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        Поставим вам обученых парикмахеров
                                    </div>
                                </div>
                            </li>
                            <li class="need-to-know__block-list-item">
                                <div class="need-to-know__block-card">
                                    <img src="img/need-to-know/2.svg" alt="" class="need-to-know__block-card-image">
                                    <div class="need-to-know__block-card-key">
                                        Экономия на открытии
                                    </div>
                                    <div class="need-to-know__block-card-value">
                                        30% скидка нак всю мебель
                                        и оборудование
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>
